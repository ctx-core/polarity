{
	"name": "@ctx-core/polarity",
	"version": "14.0.40",
	"description": "ctx-core polarity",
	"keywords": [
		"ctx-core",
		"polarity"
	],
	"homepage": "https://github.com/ctx-core/polarity#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/polarity/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/polarity/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/html": "workspace:^11.0.28",
		"@ctx-core/number": "workspace:^8.0.25"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"svelte": "3.44.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
